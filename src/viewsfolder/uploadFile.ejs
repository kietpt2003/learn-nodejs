<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/styles.css" />
    <title>Home Page</title>
</head>

<body>
    <div class="topnav">
        <a href="/">Home</a>
        <a href="/create">Create Users</a>
        <a class="active" href="/upload">Upload File</a>
    </div>
    <div class="upload-file-container">
        <form method="post" action="/upload-profile-pic" enctype="multipart/form-data">
            <div>
                <label>Avatar:</label>
                <input type="file" name="profile_pic" />
            </div>
            <div>
                <button class="btn-save" type="submit">Save files</button>
            </div>
        </form>

        <form method="post" action="/upload-multiples-images" enctype="multipart/form-data">
            <div>
                <label>Select multiple images:</label>
                <input type="file" name="multiple_images" multiple />
            </div>
            <div>
                <button class="btn-save" type="submit">Upload</button>
            </div>
        </form>
    </div>
</body>

</html>